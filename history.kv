MDScreen:
    name: 'history'
    MDFloatLayout:
        MDLabel:
            text: 'History'
            pos_hint: {"center_x": .7, "center_y": .93}
            font_size: '35sp'
            theme_text_color: "Custom"
            text_color: 0 / 255, 255 / 255, 0 / 255, 1

        Buttons:
            text: 'X'
            pos_hint: {"center_x": .5, "center_y": .06}
            on_release:
                root.manager. transition.direction = 'left'
                root.manager.current = 'main'


<Buttons@MDFillRoundFlatButton>
    user_font_size: '15sp'
    md_bg_color: 75/255, 0/255, 130/255, .5
    theme_text_color: 'Custom'
    text_color: 1,1,1,1